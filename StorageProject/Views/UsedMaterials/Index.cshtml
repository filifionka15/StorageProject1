@model IEnumerable<StorageProject.Models.UsedMaterial>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.DateOfUse)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.QuantityOfMaterial)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Materials.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Technics.Name)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.DateOfUse)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.QuantityOfMaterial)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Materials.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Technics.Name)
        </td>
        <td>

            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
